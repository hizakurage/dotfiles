# -*- mode: conf-unix -*-

[alias]
    ga = log --graph --decorate=short --oneline --all
    g  = log --graph --decorate=short --oneline
    co = checkout

[url "git@github.com:"]
    pushinsteadof = https://github.com/

[include]
    path = ~/.gitconfig.local

[core]
    excludesfile = ~/.gitignore_global
